{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "PulseGeneratorSettings": {
    "RemoteClockTimeHref": "https://fastclock.azurewebsites.net/api/clocks/demo/time",
    "PollIntervalSeconds": 2,
    "AnalogueClockStartTime": "06:00",
    "FastForwardIntervalMilliseconds": 3000,
    "PulseDurationMilliseconds": 2000,
    "Use12HourClock": true,
    "UdpBroadcast": {
      "IPAddress": "255.255.255.255",
      "PortNumber": 25501,
      "Disabled": true
    },
    "SerialPulseSink": {
      "PortName": "COM3",
      "DtrOnly": false,
      "Disabled": true
    },
    "RpiRelayBoardPulseSink": {
      "UseRelay1AsClockStatus": false
    }
  }
}
