{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Tellurian.Trains.ClockPulseApp.Service": "Debug"
    }
  },
  "PulseGeneratorSettings": {
    "ClockName": "Demo", 
    "UseRemoteClock": true, 
    "LocalServerHref": "http://localhost:5000", 
    "RemoteServerHref": "https://fastclock.azurewebsites.net", 
    "PollIntervalSeconds": 2,
    "AnalogueClockStartTime": "06:00",
    "FastForwardIntervalMilliseconds": 500,
    "PulseDurationMilliseconds": 200,
    "Use12HourClock": true,
    "FlipPolarity": false,
    "UdpBroadcast": {
      "IPAddress": "255.255.255.255",
      "PortNumber": 25501,
      "Disabled": true
    },
    "SerialPulseSink": {
      "PortName": "COM3",
      "DtrOnly": false,
      "Disabled": true
    },
    "RpiRelayBoardPulseSink": {
      "Disabled": true
    }
  }
}
